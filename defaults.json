{
    "api": {
        "url": "https://api.trello.com/1/"
    },
    "dest": {
        "root": "static",
        "all": "pages",
        "tags": "tags",
        "post": "post",
        "tag": "tag"
    },
    "fields": {
        "fields": ["id", "name", "dateLastActivity", "desc", "idList", "labels"],
        "members": true,
        "member_fields": ["fullName", "bio"],
        "attachments": true,
        "attachment_fields": ["previews", "url"]
    },
    "pagination": {
        "entriesPerPage": 20
    }
}
